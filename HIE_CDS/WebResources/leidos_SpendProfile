var Leidos = window.Leidos || {};

Leidos.setFieldsDisabled = function(executionContext) {
   var formContext =  executionContext.getFormContext()
    var vSPDetail = formContext.data.entity;
    var vLocked =  vSPDetail.Attributes.getByName("leidos_locked").getValue();
    if (vLocked == "Yes") {
        vSPDetail.attributes.forEach(function(field, i) {
            field.controls.get(0).setDisabled(true);
        });
    }
}